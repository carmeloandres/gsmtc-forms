{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "gsmtc-forms/text",
	"version": "0.1.0",
	"title": "Form Text",
	"category": "gsmtc-forms",
	"icon": "edit",
	"textdomain": "gsmtc-forms",
	"editorScript": "file:./build/index.js",
	"editorStyle": "file:./build/index.css",
	"style": "file:./build/style-index.css",
	"attributes": {
		"name":{
			"type":"string",
			"default":""
		},
		"defaultValue":{
			"type":"string",
			"default":""
		},
		"required":{
			"type":"boolean",
			"default":false
		},
		"validationMessage":{
			"type":"string",
			"default":"Letters, numbers, spaces, quotes, question marks and exclamation marks"
		}
	}
}